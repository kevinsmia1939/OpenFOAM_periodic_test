FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    wall
    {
        type            nutUSpaldingWallFunction;
        value           uniform 0;
    }
    defaultFaces
    {
        type            zeroGradient;
    }
    constraint_master
    {
        type            cyclicAMI;
        value           nonuniform List<scalar> 0();
    }
    constraint_slave
    {
        type            cyclicAMI;
        value           uniform 0;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary0to2
    {
        type            processor;
        value           uniform 0;
    }
}


