FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    wall
    {
        type            movingWallVelocity;
        value           uniform (0 0 0);
    }
    defaultFaces
    {
        type            slip;
    }
    constraint_master
    {
        type            cyclicAMI;
        value           nonuniform List<vector> 0();
        neighbourValue  nonuniform List<vector> 0();
    }
    constraint_slave
    {
        type            cyclicAMI;
        value           uniform (0 0 0);
        neighbourValue  uniform (0 0 0);
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


// ************************************************************************* //
